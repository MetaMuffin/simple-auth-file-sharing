<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>

<body>
  <h2>Contents of <%= dirname %></h2>
  <ul>
    <li>
      <a href="..">..</a>
    </li>
    <% for (const f of files) { %>
    <li>
      <a href="./<%= f.name %>"><%= f.name %></a>
      <sup><%= f.cgi ? "(cgi)" : (f.dir ? "(directory)" : "(file)") %></sup>
    </li>
    <% } %>
  </ul>
</body>

</html>